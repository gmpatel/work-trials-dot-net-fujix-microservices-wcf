  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <serviceConfig>
    <assemblies>
      <assembly name="FXA.DPSE.MessageDispatcher.DipsTransport, Version=1.0.0.0, Culture=neutral, PublicKeyToken=45ce1805ab6ffd37" />
    </assemblies>
  </serviceConfig>
  <autofac>
    <components>
      <!-- Example: How to override default Logger Component; the assembly "FXA.DPSE.Custom.Logging.DLL" does not exists -->
      <!--<component type="FXA.DPSE.Custom.Logging.Logger, FXA.DPSE.Custom.Logging, Version=1.0.0.0, Culture=neutral, PublicKeyToken=45ce1805ab6ffd37" service="FXA.DPSE.Framework.Scheduler.Service.Infrastructure.Core.ILogger, FXA.DPSE.Framework.Scheduler.Service.Infrastructure, Version=1.0.0.0, Culture=neutral, PublicKeyToken=45ce1805ab6ffd37" instance-scope="single-instance" />-->

      <!-- Inject Jobs -->
      <component name="Job1" type="FXA.DPSE.Framework.Scheduler.Service.Infrastructure.Samples.SleepJob, FXA.DPSE.Framework.Scheduler.Service.Infrastructure, Version=1.0.0.0, Culture=neutral, PublicKeyToken=45ce1805ab6ffd37" service="FXA.DPSE.Framework.Scheduler.Service.Infrastructure.Core.IMessageDispatcherJob, FXA.DPSE.Framework.Scheduler.Service.Infrastructure, Version=1.0.0.0, Culture=neutral, PublicKeyToken=45ce1805ab6ffd37">
        <metadata>
          <item name="componentType" value="IMessageDispatcherJob" type="System.String" />
          <item name="repeatFrequencyInSeconds" value="3" type="System.String" />
          <item name="repeatCount" value="1" type="System.String" />
          <item name="sleepIntervalInMiliseconds" value="3000" type="System.String" />
        </metadata>
      </component>
      <component name="Job2" type="FXA.DPSE.MessageDispatcher.DipsTransport.DipsTransportMessageDispatcherJob, FXA.DPSE.MessageDispatcher.DipsTransport, Version=1.0.0.0, Culture=neutral, PublicKeyToken=45ce1805ab6ffd37" service="FXA.DPSE.Framework.Scheduler.Service.Infrastructure.Core.IMessageDispatcherJob, FXA.DPSE.Framework.Scheduler.Service.Infrastructure, Version=1.0.0.0, Culture=neutral, PublicKeyToken=45ce1805ab6ffd37">
        <metadata>
          <item name="componentType" value="IMessageDispatcherJob" type="System.String" />
          <item name="repeatFrequencyInSeconds" value="5" type="System.String" />
          <item name="repeatCount" value="5" type="System.String" />
        </metadata>
      </component>
    </components>
  </autofac>
  <dipsTransport>
    <endPoints>
      <endPoint url="http://localhost:61822/dipstransport/post" />
    </endPoints>
  </dipsTransport>
  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogFileAppender" />
    </root>
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="C:\Temp\Log4Net\FXA.DPSE.Framework.Scheduler.Service.log" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <param name="LevelMin" value="DEBUG" />
        <param name="LevelMax" value="FATAL" />
      </filter>
    </appender>
  </log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Common.Logging.Core" publicKeyToken="af08829b84f0328e" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.2.0.0" newVersion="3.2.0.0" />
